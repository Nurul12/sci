{ TABLE "mis".cngl row size = 94 number of columns = 6 index size = 18 }
create table "mis".cngl 
  (
    cr_no char(8),
    cr_gl char(8),
    cr_cr_amt money(16,2),
    cr_dr_amt money(16,2),
    cr_desc char(30),
    cr_desc2 char(30)
  );
revoke all on "mis".cngl from "public";

create index "mis".i_cngl on "mis".cngl (cr_no);




