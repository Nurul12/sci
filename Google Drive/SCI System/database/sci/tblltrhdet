{ TABLE "mis".ltrhdet row size = 44 number of columns = 4 index size = 12 }
create table "mis".ltrhdet 
  (
    le_no integer,
    le_code char(6) not null ,
    le_var_no integer,
    le_data char(30)
  );
revoke all on "mis".ltrhdet from "public";

create index "mis".i_ltrhdet on "mis".ltrhdet (le_no);




