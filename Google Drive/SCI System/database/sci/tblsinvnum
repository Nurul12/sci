{ TABLE "mis".sinvnum row size = 30 number of columns = 7 index size = 0 }
create table "mis".sinvnum 
  (
    prefix char(3),
    prefix_cnt char(1),
    mininvno decimal(7,0),
    maxinvno decimal(7,0),
    nextinvno decimal(8,0),
    ttyid char(10),
    check char(1)
  );
revoke all on "mis".sinvnum from "public";





