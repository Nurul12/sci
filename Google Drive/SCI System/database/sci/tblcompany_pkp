{ TABLE "mis".company_pkp row size = 259 number of columns = 15 index size = 0 }
create table "mis".company_pkp 
  (
    cp_code char(6),
    cp_name char(50),
    cp_taxno char(30),
    cp_taxdate date,
    cp_taxpkp char(40),
    cp_addr1 char(50),
    cp_addr2 char(50),
    min_serie integer,
    max_serie integer,
    nex_serie integer,
    fps_pfx char(3),
    fps_prefix char(2),
    fps_min integer,
    fps_max integer,
    fps_next integer
  );
revoke all on "mis".company_pkp from "public";





